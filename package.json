    "@testing-library/svelte": "^5.3.1",
    "@vite-pwa/sveltekit": "^1.1.0",
    "fake-indexeddb": "^6.2.5",
    "jsdom": "^27.4.0",
    "svelte": "^5.33.5",
    "svelte-check": "^4.2.1",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.89.0",
